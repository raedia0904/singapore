<script type="text/javascript">
//<![CDATA[
$(document).ready(function(){
	$.post("/cmmn/timzone.do",{emblgbd:"Asia/Singapore"},setTimeList, "json");
	$.ajax({
		type: "POST", 
		url: "/cmmn/timzone.do",
		dataType:"json",
		data : "emblgbd=Asia/Singapore", // 보안문자 답변
		success: function(json) {
			$('#local_time').val(json.time);
			$('#kor_time').val(json.koreaTime);
		}, 
		error : function(json){ 
			alert("조회하지 못하였습니다."); 
			return;
		}
	});	
	
	setInterval(getOpenTime,1000);
});

function getOpenTime(){
	setTimeList();
}  

function setTimeList(){
	var time = $('#local_time').val();	  
	var koreaTime = $('#kor_time').val();
	timeZone();
	timeZoneLoc(time,koreaTime);
}
var system_seconds = 58;
var system_minute = 56;
var system_hour = 20;
function timeZone(){
	 var seoulTime1 = document.getElementById('seoulTime1');
	 var seoulTime  = document.getElementById('seoulTime');
     var korYear    = 2019;
     var korMonth   = 03;
     var korDate    = 11;
     var korHours   =  20;
     var korMinutes = 56;
     var korSeconds = 58;
     var korampm;

     if(system_hour == korHours){
 		//minutes = system_hour;
 	}else{
 		korHours = system_hour;
 	}

 	if(system_minute == korMinutes){
 		//hours = system_minute;
 	}else{
 		korMinutes = system_minute;
 	}

 	if(system_seconds == korSeconds){
 		system_seconds = system_seconds + 1;
 	}else{
 		korSeconds = system_seconds;
 		system_seconds = system_seconds + 1;
 		if(korSeconds > 59){
 			korMinutes = korMinutes + 1;
 			system_minute = system_minute + 1;
 			system_seconds = 0;
 			korSeconds = 0;
 			if(korMinutes > 59){
 				korHours = korHours + 1;
 				system_hour = system_hour + 1;
 				korMinutes = 0;
 				system_minute = 0;
 			}
 		}
 	}
     $("#sy_year").val(korYear);
     $("#sy_month").val(korMonth);
     $("#sy_day").val(korDate);
     $("#sy_hours").val(korHours);
     $("#sy_minutes").val(korMinutes);
     $("#sy_seconds").val(korSeconds);
     if(0 <= korHours && korHours < 12){
    	 korampm="<strong>AM</strong>";
    	 if (korHours == 0) korHours = 12 ;
     }else{ 
    	 korampm="<strong>PM</strong>";
    	 korHours -= 12;
    	 if (korHours == 0) korHours = 12 ;
     }
     
     if(korHours < 10 ){korHours = "0"+korHours;}
     if(korMinutes < 10){korMinutes = "0"+korMinutes;}
     if(korSeconds < 10){korSeconds = "0"+korSeconds;}
     seoulTime1.innerHTML = korYear+"."+korMonth+"."+korDate+".";
     seoulTime.innerHTML = korHours+":"+korMinutes+":"+korSeconds+" "+korampm;
}

function timeZoneLoc(time,koreaTime){
	var l_t = new Date(time);
	var k_t = new Date(koreaTime);
	var differ = (l_t - k_t)  / (1000*60*60);
    if(k_t.getMinutes() <= 30)
    	var differM =(k_t.getMinutes() - l_t.getMinutes());
    else
    	var differM =(l_t.getMinutes() - k_t.getMinutes());

	 var localTime1 = document.getElementById('localTime1');
	 var localTime  = document.getElementById('localTime');
     var locYear    = $("#sy_year").val(); 
     var locMonth   = $("#sy_month").val(); 
     var locDate    = $("#sy_day").val();   
     var locHours   = $("#sy_hours").val(); 
     var locMinutes = $("#sy_minutes").val();
     var locSeconds = $("#sy_seconds").val();
	 var locampm;

     locHours = (parseInt(locHours,10) + parseInt(differ,10));
     
     if (locHours < 0)
    {
     locHours = locHours + 24;
     locDate = locDate - 1;
    }
     if (locHours > 23)
     {
      locHours = locHours - 24;
      locDate = locDate + 1;
     }
   
     locMinutes = (parseInt(locMinutes,10) + parseInt(differM,10));
     if (locMinutes < 0)
     {
      locMinutes = locMinutes + 60;
      locHours = locHours - 1;
     }

     if (locMinutes > 59)
     {
      locMinutes = locMinutes - 60;
      locHours = locHours + 1;
     }
    
     if(0 <= locHours && locHours < 12){
    	 locampm="<strong>AM</strong>";
    	 if (locHours == 0) locHours = 12 ;
     }else{
    	 locampm="<strong>PM</strong>";
    	 locHours -= 12;
    	 if (locHours == 0) locHours = 12 ;
     }
     if(locDate <=0 ) {
    	 var leapyear="";
    	 locMonth = locMonth - 1;
			if(locMonth==1||locMonth==3||locMonth==5||locMonth==7||locMonth==8||locMonth==10||locMonth==12)
				locDate = 31 + locDate ;
			else if(locMonth==4||locMonth==6||locMonth==9||locMonth==11)
				locDate = 30 + locDate ;
			else if(locMonth==2 && leapyear=="true")
				locDate = 29 + locDate ;
			else if(locMonth==2 && leapyear!="true")
				locDate = 28 + locDate ;
			else if(locMonth<=0) {
				locYear = locYear - 1;
				locMonth = 12;
				locDate = 31;
			}
		}
     
     
     if(locHours < 10){locHours = "0"+locHours;}
     if(locMinutes < 10){locMinutes = "0"+locMinutes;}
     if(locSeconds < 10){locSeconds = "0"+locSeconds;}
     localTime1.innerHTML = locYear+"."+locMonth+"."+locDate+".";
     if(isNaN(locHours)){	 
     }else{
     localTime.innerHTML =locHours+":"+locMinutes+":"+locSeconds+" "+locampm;
     }

}
var xpos = 0;
var ypos = 0;

$(function() { //공통 팝업

	
	if(isWinOpen("notOpenWin173")){
		fn_popup("/com/popup/view.do?seq=173&site_itm=3", 'win173' ,'350', '380', "yes",xpos,ypos);
		xpos = xpos + 350 + 20;
	}
	
});


