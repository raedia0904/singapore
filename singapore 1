<script type="text/javascript">
//<![CDATA[
	$(document).ready(function(){
		getOpenAbroadList($('#cntt').val());
		$("#cntt").append("<option value=''>:::대륙선택:::</option>");
		$("#emblgbd").append("<option value=''>:::공관선택:::</option>");
	});
	
	//대륙코드선택 selectBox
	function getOpenAbroadList(_cntt){
		//재외공관한글명바로가기 abroadKo
		if(_cntt != "")
		$.post("/cmmn/abroadKoSelectBox.do",{cntt : _cntt},setOpenAbroadList, "json");
	}
	
	//공관선택 selectBox
	function setOpenAbroadList(data, status){
		if ( 'success' != status || null == data ) { alert("조회하지 못하였습니다."); return; }
		var obj = $('select[name="emblgbd"]');
		obj.children().remove();
		$("#emblgbd").append("<option value=''>:::공관선택:::</option>");
		if(data.vo.length == 7){
		for(var i =0; i < data.vo.length; i++){
			$("#cntt").append("<option value="+data.vo[i].code+">"+data.vo[i].description+"</option>");
		}
		}else {
			for(var i =0; i < data.vo.length; i++){
				$("#emblgbd").append("<option value="+data.vo[i].deptNm+">"+data.vo[i].description+"</option>");
			}
		}
		   	
	}
	//공관링크바로가기
	function goabroad(url){
		var chkurl="";
		if(url != chkurl){window.open(url);	return;}
	}
//]]> 
</script>
	
